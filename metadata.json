{
  "name": "cirrax-doas",
  "version": "1.0.4",
  "author": "Cirrax GmbH",
  "summary": "configure doas on OpenBSD (equivalent of sudo)",
  "license": "GPL-3.0",
  "source": "https://github.com/cirrax/puppet-doas",
  "project_page": "https://github.com/cirrax/puppet-doas",
